{
  "expo": {
    "name": "MASLOW",
    "slug": "maslow-app",
    "version": "2.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "maslow",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#F9F2EC"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.maslownyc.app",
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#F9F2EC"
      },
      "package": "com.maslownyc.app",
      "versionCode": 1
    },
    "plugins": [
      "expo-router",
      "@react-native-community/datetimepicker",
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Maslow to access your photos to upload profile pictures"
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "975dcf24-c4c2-461e-a17f-03ac527ac570"
      }
    }
  }
}
