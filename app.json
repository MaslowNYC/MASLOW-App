
{
  "expo": {
    "name": "Maslow Pass",
    "slug": "maslow-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "maslow",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#F9F2EC"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.maslownyc.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#F9F2EC"
      },
      "package": "com.maslownyc.app"
    },
    "plugins": ["expo-router"]
  }
}